# Choose provider: gemini or openai
AI_PROVIDER=gemini

# Beefree SDK credentials
BEEFREE_CLIENT_ID=your_beefree_client_id
BEEFREE_CLIENT_SECRET=your_beefree_client_secret
BEEFREE_MCP_API_KEY=your_beefree_mcp_api_key

# Gemini
GEMINI_API_KEY=your_gemini_api_key
LLM_MODEL=gemini-2.5-pro

# OpenAI (switch AI_PROVIDER to openai and set the model)
OPENAI_API_KEY=your_openai_api_key
# Example OpenAI model:
# LLM_MODEL=gpt-5.2

# App
APP_HOST=0.0.0.0
APP_PORT=8000